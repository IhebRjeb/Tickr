{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/setup.ts"],
  "globalSetup": "<rootDir>/helpers/test-database.helper.ts",
  "globalTeardown": "<rootDir>/helpers/test-database.helper.ts",
  "testTimeout": 30000
}
